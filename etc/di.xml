            <argument name="commands" xsi:type="array">
                <item name="yireo_show_last_sku" xsi:type="object">Yireo\ExampleIncrementalSku\Console\Command\ShowLastSkuCommand</item>
                <item name="yireo_show_next_sku" xsi:type="object">Yireo\ExampleIncrementalSku\Console\Command\ShowNextSkuCommand</item>
            </argument>
        </arguments>
    </type>
</config>
